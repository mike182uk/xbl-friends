{
  "name": "xbl-friends",
  "scripts": {
    "dev-server": "NODE_ENV=dev DEV_SERVER_PORT=3000 webpack-dev-server --dev --colors --hot --inline --port 3000 --content-base ./src/app --config ./webpack.app.dev.config.js",
    "dev-app": "NODE_ENV=dev DEV_SERVER_PORT=3000 electron src/electron/index.js",
    "dev-server-no-xbl": "NO_XBL=true npm run dev-server",
    "dev-app-no-xbl": "NO_XBL=true npm run dev-app",
    "dev-app-int": "NODE_ENV=dev DEV_SERVER_PORT=3000 electron src/electron/index.js",
    "dev-app-int-no-xbl": "NO_XBL=true npm run dev-app-int",
    "build": "rm -rf ./build/* && BABEL_ENV=prod webpack --config ./webpack.app.config.js && cp -r ./{src/electron/*,src/common,package.json} ./build/",
    "built-app": "electron ./build/index.js",
    "package": "rm -rf ./dist/* && electron-packager ./build --platform darwin --arch x64 --out ./dist --icon ./build/xbl-friends.icns --asar"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "electron-packager": "^6.0.1",
    "electron-prebuilt": "^0.36.4",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.7.1",
    "postcss-cssnext": "^2.4.0",
    "postcss-loader": "^0.8.0",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.0",
    "redux-devtools": "^3.0.2",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-thunk": "^1.0.3",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "deep-assign": "^2.0.0",
    "font-awesome": "^4.5.0",
    "immutable": "^3.7.6",
    "lodash": "^4.2.1",
    "moment": "^2.11.1",
    "react": "^0.14.6",
    "react-bootstrap": "^0.28.2",
    "react-dom": "^0.14.6",
    "react-fontawesome": "^0.3.3",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc5",
    "react-router-bootstrap": "^0.20.1",
    "react-router-redux": "^2.1.0",
    "redux": "^3.0.5",
    "redux-storage": "^3.0.0",
    "redux-storage-decorator-filter": "^1.0.0",
    "redux-storage-engine-localstorage": "^1.0.0",
    "validator": "^5.2.0"
  }
}
